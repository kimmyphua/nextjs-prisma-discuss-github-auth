{
	"name": "discuss",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "dotenv -e .env.local -- next dev",
		"build": "prisma generate migrate deploy && next build",
		"start": "next start",
		"lint": "next lint",
		"start:db": "docker run --rm -p 5431:5432 -v postgres-data:/var/lib/postgresql/data -e POSTGRES_HOST_AUTH_METHOD=trust postgres",
		"prisma:migrate:local": "dotenv -e .env.local -- npx prisma migrate dev --schema=prisma/schema.prisma",
		"prisma:generate:local": "dotenv -e .env.local -- npx prisma generate --schema=prisma/schema.prisma",
		"postinstall": "prisma generate"
	},
	"dependencies": {
		"@auth/core": "0.18.1",
		"@auth/prisma-adapter": "1.0.6",
		"@nextui-org/react": "2.2.9",
		"@prisma/client": "^5.21.1",
		"@vercel/postgres": "^0.10.0",
		"dotenv-cli": "^7.4.2",
		"framer-motion": "11.11.9",
		"next": "14.2.15",
		"next-auth": "5.0.0-beta.3",
		"prisma": "^5.21.1",
		"react": "^18",
		"react-dom": "^18",
		"sass": "^1.80.4",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"eslint": "^8",
		"eslint-config-next": "14.2.15",
		"eslint-import-resolver-node": "^0.3.9",
		"eslint-plugin-css-modules": "^2.12.0",
		"postcss": "^8",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	}
}
